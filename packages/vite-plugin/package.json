{
  "name": "vite-plugin-svelte-svelterial",
  "version": "0.0.8",
  "description": "A plugin for integration with Vite and SvelteKit.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "umd:main": "dist/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.modern.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "microbundle src/index.js",
    "dev": "microbundle watch src/index.js",
    "test": "uvu -r esm test"
  },
  "keywords": [
    "vite-plugin",
    "svelte",
    "svelterial"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svelterialjs/integrations.git",
    "directory": "packages/vite-plugin"
  },
  "author": "TheComputerM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/svelterialjs/integrations/issues"
  },
  "homepage": "https://github.com/svelterialjs/integrations/tree/main/packages/vite-plugin#readme",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "svelte": "^3.38.0",
    "vite": "^2.2.4"
  },
  "dependencies": {
    "sass": "^1.34.0"
  },
  "devDependencies": {
    "@svelterialjs/plugin-utils": "workspace:*",
    "esm": "^3.2.25",
    "microbundle": "^0.13.1",
    "uvu": "^0.5.1"
  }
}
