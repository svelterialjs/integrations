{
  "name": "vite-plugin-svelte-svelterial",
  "version": "0.0.5",
  "description": "A plugin for integration with Vite and SvelteKit.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "node": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "uvu -r esm test"
  },
  "keywords": [
    "vite-plugin",
    "svelte",
    "svelterial"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svelterialjs/integrations.git",
    "directory": "packages/vite-plugin"
  },
  "author": "TheComputerM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/svelterialjs/integrations/issues"
  },
  "homepage": "https://github.com/svelterialjs/integrations/tree/main/packages/vite-plugin#readme",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "svelte": "^3.38.0",
    "vite": "^2.2.4"
  },
  "dependencies": {
    "sass": "^1.32.12"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@svelterialjs/plugin-utils": "workspace:*",
    "esm": "^3.2.25",
    "rollup": "^2.46.0",
    "rollup-plugin-terser": "^7.0.2",
    "uvu": "^0.5.1"
  }
}
